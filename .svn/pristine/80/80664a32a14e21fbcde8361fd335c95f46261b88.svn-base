body{background: #F0F0F0;}
.sidebar{background:#F0F0F0;-webkit-box-shadow: #666 3px 0px 10px;box-shadow: #666 3px 0px 10px;width:180px;}
.sidebar-nav{background:#118DC9;}
.sidebar-nav ul a{background:#4AB6E7;color:white;border-bottom:1px solid #E2E2E2;font-size:14px;text-align:left;}
.sidebar-nav ul ul a{background:#F0F0F0;color:gray;font-size:14px;border-bottom:2px solid white;}
.sidebar-nav ul ul a:hover, .sidebar-nav ul ul a:focus, .sidebar-nav ul ul a:active{background:#DCDCDC;color:#5CB0D5;}
.common_content{background:#F5F5F5;float:left;font-size:12px;padding-bottom:30px;margin-left: 8px;width:80%;}
.common_content .data_div_header{background: url(../img/location.png) no-repeat 0px center; width:95%;height:40px;font-family:黑体;font-size:14px;margin-left:20px;text-indent:20px;line-height:40px;float:left;border-bottom:2px solid #D7D7D7;}
.common_content .data_div_condition{width:95%;height:120px;margin-left:20px;margin-top:15px;float:left;
	background:white;border:2px solid #D7D7D7;-webkit-box-shadow: #666 0px 3px 10px;box-shadow: #666 0px 3px 10px;}
.common_content .data_div_condition .condition_table{width:98%;height:100%;font-size:12px;cellspacing:2px;margin-left:6px;}
.common_content .data_div_datalist{width:95%;float:left;margin-left:20px;margin-top:15px;-webkit-box-shadow: #666 0px 3px 10px;box-shadow: #666 0px 3px 10px;    padding-bottom: 6px;}

.data_div_datalist #apply_table_wrapper{width:100%;}
.data_div_datalist #apply_table_wrapper .row .col-sm-12{padding:0;marign:0;}
.data_div_datalist #apply_table_wrapper .row .col-sm-12 table .dataTable{margin:0px;}
table.dataTable th{color:white;font-size:14px;}

table.dataTable td{border:1px solid #E1E1E1;}

.td-text-align-right{text-align:right;}
.td-text-align-left{text-align:left;}

select option{font-size:12px;}

.dataTable tbody td p{display: block;width: 73px;min-width: 50px;margin: 2px auto;background: #4ab6e7;padding: 5px;border-radius: 4px;color: #fff;}
.dataTable tbody td .accept{background: #ED9863;}
.dataTable tbody td .accept:hover{background: #EC7B35;}


.yl:hover{background:#00abf9;}

.metismenu li a{
	white-space:nowrap; overflow:hidden; text-overflow:ellipsis;
}

.hr-user-box{height:50px;line-height:50px;float:right;margin-right:5px;}
.hr-user-box-username{height:50px;line-height:50px;color:#fff;float:left;}
.hr-user-box-photo{width:30px;height:30px; margin:10px;border-radius:15px;background:#fff;float:left;}
.hr-user-box-logout{width:18px;height:18px;margin:16px 5px 16px 10px;}
.text-align-left{width: 98%;text-align: left;display: block;margin-left: 1%;}
.btn{margin-left: 0px 10px;margin-right:10px;}

