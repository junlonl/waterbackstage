}


.text_username_container{
	width:350px;
	height:45px;
	border-radius:10px;
	border:2px solid #E5E5E5;
	text-align:right;
	background:url(../img/login/username_gray.png) no-repeat 10px center;
	float:left;
	margin:30px 0 0 30px;
}
.text_username_container .text_username{
	width:300px;
	height:40px;
	border-radius:10px;
	border:0px;	
	text-indent:10px;
}
