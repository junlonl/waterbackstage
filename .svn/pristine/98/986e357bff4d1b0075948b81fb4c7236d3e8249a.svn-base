.header{
	background:white;
	height:50px;
	width:100%;
	border-bottom:2px solid #3EDBE4;
}
.header .header_content{
	width:1100px;
	height:100%;
	margin:0 auto;		
}
.header .header_content .logo{
	background:url("../img/login/logo.png") no-repeat 0px 6px;
	height:48px;
	width:300px;
	float:left;
}
.header .header_content .header_wechat{
	background:url("../img/login/wechat_logo.png") no-repeat 0px 6px;
	height:48px;
	width:130px;
	float:right;
	
}

.container{
	background:url("../img/login/header_bg.png") no-repeat; 
	width:100%;
	height:500px;
	z-index:1;
	-webkit-box-shadow: #666 0px 3px 10px;
	box-shadow: #666 0px 3px 10px;
	margin:0 auto;
}
.container_content_div{
	width:950px;
	height:100%;
	margin:0 auto;
	padding-top:130px;
	padding-left:600px;
	background:url('../img/login/container_bg.png') no-repeat left 0;
	
}
